import random
import numpy as np
matrix =[["a", "b", 2],
    ["b", "c", 3],
    ["a", "c", 4],
    ["a", "d", 5],
    ["c", "d", 6]]
# candidate = np.sort(matrix)
def sort(matrix):
    num = sorted([i for i in matrix if isinstance(i,int)])
    char = sorted([i for i in matrix if isinstance(i,str)])
    return char + num
    print("sort",sort)
sort()
